        android:textColor="#ffffff"
        android:textSize="25sp" />

    <Button
        android:id="@+id/Hight_Math_button_Time_up"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/hightextView5"
